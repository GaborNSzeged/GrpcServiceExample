{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://0.0.0.0:5001"
      }
    },
    "ServerSettings": {
      "ApplicationUrl": "http://new-url.com"
    }
  },
  "LoggerConfig": {
    "LogFilesDirectory": "c:/Temp2"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
